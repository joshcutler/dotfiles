[hub]
        protocol = https
[user]
	name = Josh Cutler
	email = josh.cutler@gmail.com
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
        editor = vim
[apply]
        whitespace = nowarn
[mergetool]
	keepBackup = false
[difftool]
        prompt = false
[help]
        autocorrect = 1
[push]
	default = matching
[alias]
	up = "!git remote update -p; git merge --ff-only @{u}"
